{
	"name": "vscode-test-web-fs",
	"displayName": "vscode-test-web file system provider",
	"version": "0.0.1",
	"private": false,
	"description": "Provides a file provider for web tests to access local files and folders.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"browser": "./dist/fsExtensionMain.js",
	"scripts": {
		"compile-web": "webpack",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run package-web",
		"watch-web": "webpack --watch"
	},
	"contributes": {
		"resourceLabelFormatters": [
			{
				"authority": "mount",
				"formatting": {
					"label": "${path}",
					"workspaceSuffix": "Test Files"
				},
				"scheme": "vscode-test-web"
			}
		]
	},
	"activationEvents": [
		"onFileSystem:vscode-test-web",
		"onSearch:vscode-test-web"
	],
	"devDependencies": {
		"@types/vscode": "^1.81.0",
		"minimatch": "9.0.3",
		"path-browserify": "^1.0.1",
		"process": "^0.11.10",
		"request-light": "^0.7.0",
		"ts-loader": "^9.5.1",
		"typescript-esbuild": "0.4.5",
		"vscode-uri": "^3.0.8",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": [
		"fileSearchProvider"
	]
}
