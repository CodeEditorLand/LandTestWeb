	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"browser": "./dist/web/extension.js",
	"scripts": {
		"compile-web": "webpack",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run package-web",
		"watch-web": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "vscode-test-web-sample.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vscode-test-web-sample.findFiles",
				"title": "Find files"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/mocha": "10.0.6",
		"@types/vscode": "^1.81.0",
		"@types/webpack-env": "^1.18.4",
		"assert": "^2.1.0",
		"mocha": "^10.2.0",
		"process": "^0.11.10",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"typescript-esbuild": "0.4.5",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
